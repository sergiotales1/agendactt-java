/**
 * validacao de formulario
 */

 function validar(){
	 let nome = frmContato.nome.value
	 let fone = frmContato.fone.value
	 
	 if(nome === ""){
		 alert('Preencha o campo Nome')
		 fmrContato.nome.focus()
		 return false
	 }
	 else if(nome === ""){
		 alert('Preencha o campo Nome')
		 fmrContato.nome.focus()
		 return false
	 }
 }